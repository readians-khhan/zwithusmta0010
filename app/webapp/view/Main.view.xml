<mvc:View controllerName="withus.sci.management.SCIManagement.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" 
xmlns:core="sap.ui.core" xmlns:t="sap.ui.table" xmlns:f="sap.ui.layout.form" xmlns="sap.m" xmlns:tnt="sap.tnt" 
xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<tnt:ToolPage class="sapUiDemoToolPage" id="idToolPageMain" sideExpanded="{mainView>/sideExpanded}">
		<tnt:header>
			<!--페이지 헤더 -->
			<tnt:ToolHeader>
				<!--화면 로고! -->
				<html:div class="sk_siltron_logo"/>
				<Text text="{i18n>tlMainHeadTitle}" />
				<ToolbarSpacer />
				<Button visible="true" class="sapUiTinyMarginEnd" type="Default" text="{i18n>Login}" press="onPAI" app:fcCode="fcLogout" />
			</tnt:ToolHeader>
		</tnt:header>
		<!-- Side Menu -->
		<tnt:sideContent class="side-navigation">
			<tnt:SideNavigation id="sideNavigation" selectedKey="subItem3">
				<tnt:NavigationList>
					<tnt:NavigationListItem text="{i18n>SideInterfaceList}" icon="sap-icon://employee"></tnt:NavigationListItem>
				</tnt:NavigationList>
				<tnt:fixedItem>
					<tnt:NavigationList>
						<tnt:NavigationListItem text="{i18n>sideCdodeList}" icon="sap-icon://employee" />
						<tnt:NavigationListItem text="{i18n>sideSystemList}" icon="sap-icon://building" />
					</tnt:NavigationList>
				</tnt:fixedItem>
			</tnt:SideNavigation>
		</tnt:sideContent>

	</tnt:ToolPage>
</mvc:View>