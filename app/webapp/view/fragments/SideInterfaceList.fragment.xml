<core:FragmentDefinition xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:t="sap.ui.table"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <t:Table id="tabInterfaceList"
        selectionMode="Single"
        enableSelectAll="false"
        alternateRowColors="true"
        visibleRowCountMode="Auto"
        enableBusyIndicator="true"
        rows="{
            path: 'management>/SCI_TP_INTERFACELIST_SRV',
            parameters: {   
                $count: true,
                $$updateGroupId: 'InterfaceDataGroup'
            }
        }"
        rowSelectionChange="onPAI"
        app:fcCode="fcFileSelectionChange">
        <t:extension>
            <OverflowToolbar>
                <Title text="{i18n>tlTotalCount} { 
                    path: 'mainView>/Files/totalCount',
                    type: 'sap.ui.model.odata.type.Int32'}"
                    wrapping="false" />
                <Button type="Transparent"
                    icon="sap-icon://refresh"
                    press="onPAI"
                    app:fcCode="fcFilesRefresh" />
                <ToolbarSpacer/>
                <Button text="{i18n>btnCreate}"
                    icon="sap-icon://create"
                    press="onPAI"
                    app:fcCode="fcCreateIntefaceList"/>
                <Button text="{i18n>btnDelete}"
                    icon="sap-icon://delete"
                    type="Reject"
                    enabled="{= ${mainView>/Files/selectedCount} > 0 ? true : false}"
                    press="onPAI"
                    app:fcCode="fcDeleteInterface"/>
                <Button icon="sap-icon://download"
                    text="{i18n>btnDownload}"
                    type="Emphasized"
                    press="onPAI"
                    app:fcCode="fcDownloadIntefaceList"
                    busy="{mainView>/iconBusy}"/>
            </OverflowToolbar>
        </t:extension>
        <t:columns>
            <!-- Deleted_TF -->
            <t:Column width="7rem"
                autoResizable="true"
                sortProperty="DELEDTED_TF"
                filterProperty="DELEDTED_TF"
                hAlign="Center">
                <Label text="{i18n>fldDelted}"
                    textAlign="Center"
                    width="100%"/>
                <t:template>
                    <Text text="{management>DELEDTED_TF}"
                        textAlign="Begin"
                        width="100%"
                        wrapping="false"/>
                </t:template>
            </t:Column>
            <!-- Interface Status -->
            <t:Column width="7rem"
                autoResizable="true"
                sortProperty="STATUS_DESC01"
                filterProperty="STATUS_DESC01"
                hAlign="Center">
                <Label text="{i18n>fldStatus}"
                    textAlign="Center"
                    width="100%"/>
                <t:template>
                    <Text text="{management>STATUS_DESC01}"
                        textAlign="Begin"
                        width="100%"
                        wrapping="false"/>
                </t:template>
            </t:Column>
            <!-- Interface No -->
            <t:Column width="7rem"
                autoResizable="true"
                sortProperty="IF_NO"
                filterProperty="IF_NO"
                hAlign="Center">
                <Label text="{i18n>fldInterfaceNumber}"
                    textAlign="Center"
                    width="100%"/>
                <t:template>
                    <Text text="{management>IF_NO}"
                        textAlign="Begin"
                        width="100%"
                        wrapping="false"/>
                </t:template>
            </t:Column>
            <!-- Interface Name -->
            <t:Column width="7rem"
                autoResizable="true"
                sortProperty="IF_NM"
                filterProperty="IF_NM"
                hAlign="Center">
                <Label text="{i18n>fldInterfaceName}"
                    textAlign="Center"
                    width="100%"/>
                <t:template>
                    <Text text="{management>IF_NM}"
                        textAlign="Begin"
                        width="100%"
                        wrapping="false"/>
                </t:template>
            </t:Column>
            <!-- 갱신날짜 -->
            <t:Column width="7rem"
                autoResizable="true"
                sortProperty="IF_DESC"
                filterProperty="IF_DESC"
                hAlign="Center">
                <Label text="{i18n>fldmodifiedAt}"
                    textAlign="Center"
                    width="100%"/>
                <t:template>
                    <Text text="{management>modifiedAt}"
                        textAlign="Begin"
                        width="100%"
                        wrapping="false"/>
