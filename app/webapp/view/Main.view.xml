<mvc:View controllerName="withus.sci.management.SCIManagement.controller.Main"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns:core="sap.ui.core"
	xmlns:t="sap.ui.table"
	xmlns:f="sap.ui.layout.form"
	xmlns="sap.m"
	xmlns:tnt="sap.tnt"
	xmlns:l="sap.ui.layout"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<tnt:ToolPage class="sapUiDemoToolPage" id="idToolPageMain" sideExpanded="{mainView>/sideExpanded}">
	<tnt:header>
      <tnt:ToolHeader>
        <html:div class="sk_siltron_logo" />
        <Text text="{i18n>pageHeadTitle}"/>
        <ToolbarSpacer />
        <Button visible="false"
          class="sapUiTinyMarginEnd"
          type="Default"
          text="{i18n>btnLogout}"
          press="onPAI"
          app:fcCode="fcLogout"/>
      </tnt:ToolHeader>
    </tnt:header>


	</tnt:ToolPage>
</mvc:View>